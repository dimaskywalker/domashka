def fibonacci(n): # Создание функции, которая принимает один аргумент n.
    if n <= 0:
        return "Введите число больше 0" # Проверка, если n меньше или равно 0, то возвращается сообщение "Введите число больше 0".
    elif n == 1:
        return 0 # Задаём первое число ряда Фибоначчи.
    elif n == 2:
        return 1 # Задаём второе число число ряда Фибоначчи.
    else:
        return fibonacci(n-1) + fibonacci(n-2) # Вычисление числа Фибоначчи.

f = fibonacci(int(input())) # Переменная для ввода задаваемого ряда.
print(f"Искомое число ряда Фибоначчи: {f}") # Вывод искомого числа из ряда Фибоначчи.
